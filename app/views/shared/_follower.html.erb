<% @userfeed[:followers].each do |member| %>

<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 nopaddding">
                <div id="main" class="div-main-user-pin">
                    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8 nopadding" id="first">
                        <div id="user-details" class="div-user-details">
                            <div class="col-md-4 col-sm-4 col-xs-4 nopadding" id="user-pic">
                                <div id="image-holder" class="div-image-holder"><img class="img-circle image-user-pic" src="assets/img/Garden & Patio.jpg" width="50px"></div>
                            </div>
                            <div class="col-md-8 col-sm-8 col-xs-8 nopadding" id="user-name">
                                <div class="div-user-name">
                                    <h4 class="text-left user-name-style"><strong><%= member.name %></strong></h4>
                                    <h5 class="text-left user-username"><%= member.uname %> </h5></div>
                            </div>
                        </div>
                        <div id="user-stats" class="div-user-stats">
                            <div class="col-md-4 col-sm-4 col-xs-4 nopadding" id="followers-count">
                                <div id="stat-count" class="div-stat-count">
                                    <h5 class="text-center stat-count-style"><%= member.followers.count %> </h5></div>
                                <div id="stat-name" class="div-stat-name">
                                    <h6 class="text-center stat-name-style">Followers </h6></div>
                            </div>
                            <div class="col-md-4 col-sm-4 col-xs-4 nopadding" id="followers-count">
                                <div id="stat-count" class="div-stat-count">
                                    <h5 class="text-center stat-count-style"><%= member.clubquestions.count %> </h5></div>
                                <div id="stat-name" class="div-stat-name">
                                    <h6 class="text-center stat-name-style">Questions </h6></div>
                            </div>
                            <div class="col-md-4 col-sm-4 col-xs-4 nopadding" id="followers-count">
                                <div id="stat-count" class="div-stat-count">
                                    <h5 class="text-center stat-count-style"><%= member.answers.count %>  </h5></div>
                                <div id="stat-name" class="div-stat-name">
                                    <h6 class="text-center stat-name-style">Answers </h6></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-4" id="last">
                        <div id="follow-btn" class="div-follow-btn">
                            <% if user_signed_in? %>

                                    <% if member.id==current_user.id %>
                                            

                                       
                                    <% else %>

                                     <%if current_user_is_following(current_user.id, member.id) %>

                                                <%= render partial: 'shared/user_unfollow', :locals=>{:user=>member, :uname=>member.uname} %>

                                       <% else %> 
                                                 <%= render partial: 'shared/user_follow', :locals=>{:user=>member,:uname=>member.uname} %>  

                                        <% end %>

                                
                                    <% end %>

                             <% else %>       

                            <button class="btn btn-default user-follow-btn" type="button">Follow </button>
                            <% end %>    

                        </div>
                    </div>
                </div>
            </div>

<% end %>            